    {
        "id": "4fda3be8cc6ffd97",
        "type": "function",
        "z": "8e57b4bfe258b915",
        "name": "Get Element from inputNumber",
        "func": "let arr = msg.payload;\nlet i = env.get('inputNumber');\n\nif (Array.isArray(arr) && i > 0 && i <= arr.length) {\n    msg.payload = arr[i - 1];\n} else {\n    msg.payload = `Waveshare Input Monitor ${i}: Invalid index or payload is not an array`;\n}\n\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 370,
        "y": 180,
        "wires": [
